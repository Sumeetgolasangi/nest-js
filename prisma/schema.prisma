datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Crud {
  id   Int    @id @default(autoincrement())
  name String

  @@map("Crud")
}

model User {
  id        Int     @default(autoincrement()) @id
  email     String  @unique
  name      String?
  password  String
}
